        "helmich/typo3-typoscript-parser": "1.1.*",
        "squizlabs/php_codesniffer": "2.8.*",
        "symfony/yaml": "3.2.*"
    },
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Daniel Siepmann",
            "email": "coding@daniel-siepmann.de"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "symfony/finder": "^3.2"
    }
}
